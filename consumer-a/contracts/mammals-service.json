{
    "consumer-a": {
        "mammals-service": {
            "get mammals": {
                "All the mammals are returned": {
                    "request": {
                        "baseUrl": "http://localhost:8085",
                        "path": "/api/mammals",
                        "method": "GET",
                        "timeout": 1000
                    },
                    "response": {
                        "statusCode": 200,
                        "body": [
                            {
                                "familyName": "Joi.string()",
                                "familyCommonName": "Joi.string()",
                                "id": "Joi.number().integer()",
                                "speciesUrl": "Joi.string()"
                            }
                        ]
                    }
                }
            }
        }
    }
}